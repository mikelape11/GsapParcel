{
  "name": "frontend",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "sass": "^1.29.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production parcel build index.html --dist-dir dist --no-source-maps",
    "build-yarn": "yarn css && yarn js",
    "prebuild": "rm -fr .parcel-cache dist || exit 0",
    "prebuild-yarn": "rm -rv dist",
    "prestart": "rm -fr .parcel-cache dist",
    "start": "cross-env NODE_ENV=development parcel index2.html --dist-dir dist --no-autoinstall --no-source-maps",
    "css": "sass input.scss dist/style.css",
    "js": "babel . --ignore node_modules --out-dir dist"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "parcel": "^2.0.0-beta.1"
  },
  "author": "",
  "description": ""
}
